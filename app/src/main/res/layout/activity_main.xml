<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_multiplo"
        android:layout_width="86dp"
        android:layout_height="58dp"
        android:layout_marginStart="184dp"
        android:layout_marginTop="96dp"
        android:text="*"
        android:textSize="34sp"
        app:layout_constraintStart_toEndOf="@+id/btn_limpiar"
        app:layout_constraintTop_toBottomOf="@+id/textview" />

    <Button
        android:id="@+id/button121"
        android:layout_width="104dp"
        android:layout_height="83dp"
        android:text="0"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="632dp" />

    <Button
        android:id="@+id/btn_divisor"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="/"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="313dp"
        tools:layout_editor_absoluteY="323dp" />

    <Button
        android:id="@+id/btn_resta"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="-"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="313dp"
        tools:layout_editor_absoluteY="421dp" />

    <Button
        android:id="@+id/btn_suma"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="+"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="313dp"
        tools:layout_editor_absoluteY="519dp" />

    <Button
        android:id="@+id/btn_siete"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="7"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="323dp" />

    <Button
        android:id="@+id/btn_uno"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="1"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="519dp" />

    <Button
        android:id="@+id/btn_cuatro"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="4"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="421dp" />

    <Button
        android:id="@+id/btn_dos"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="2"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="519dp" />

    <Button
        android:id="@+id/btn_ocho"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="8"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="323dp" />

    <Button
        android:id="@+id/btn_cinco"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="5"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="112dp"
        tools:layout_editor_absoluteY="421dp" />

    <Button
        android:id="@+id/btn_nueve"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="9"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="298dp" />

    <Button
        android:id="@+id/btn_tres"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="3"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="519dp" />

    <Button
        android:id="@+id/btn_seis"
        android:layout_width="93dp"
        android:layout_height="83dp"
        android:text="6"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="421dp" />

    <Button
        android:id="@btn_backspace"
        android:layout_width="133dp"
        android:layout_height="55dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:text="←"
        android:textSize="48sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview" />

    <Button
        android:id="@+id/btn_limpiar"
        android:layout_width="103dp"
        android:layout_height="62dp"
        android:layout_marginStart="168dp"
        android:layout_marginTop="24dp"
        android:text="CE"
        android:textSize="34sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview" />

    <TextView
        android:id="@+id/textview"
        android:layout_width="373dp"
        android:layout_height="104dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="40dp"
        android:text="0"
        android:textAlignment="textEnd"
        android:textSize="60sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_punto"
        android:layout_width="109dp"
        android:layout_height="82dp"
        android:text="."
        android:textSize="60sp"
        tools:layout_editor_absoluteX="7dp"
        tools:layout_editor_absoluteY="632dp" />

    <Button
        android:id="@+id/button56"
        android:layout_width="163dp"
        android:layout_height="80dp"
        android:text="="
        android:textSize="34sp"
        tools:layout_editor_absoluteX="243dp"
        tools:layout_editor_absoluteY="635dp" />

</androidx.constraintlayout.widget.ConstraintLayout>